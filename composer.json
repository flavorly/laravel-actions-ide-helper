{
    "name": "flavorly/laravel-actions-ide-helper",
    "description": "Generate a new IDE Helper file for Laravel Actions.",
    "keywords": [
        "Wulfheart",
        "Flavorly",
        "laravel",
        "laravel-actions-ide-helper"
    ],
    "homepage": "https://github.com/flavorly/laravel-actions-ide-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Wulf",
            "email": "dev@alexfwulf.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "*",
        "illuminate/contracts": "^v11.7.0",
        "lorisleiva/laravel-actions": "^2.8",
        "lorisleiva/lody": "^0.5.0",
        "phpdocumentor/reflection": "^5.3",
        "riimu/kit-pathjoin": "^1.2",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "brianium/paratest": "^7.4",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.34",
        "phpunit/phpunit": "^10.5",
        "spatie/invade": "^2.0",
        "spatie/laravel-ray": "^1.36",
        "spatie/pest-plugin-snapshots": "^2.1",
        "vimeo/psalm": "^5.24"
    },
    "autoload": {
        "psr-4": {
            "Wulfheart\\LaravelActionsIdeHelper\\": "src",
            "Wulfheart\\LaravelActionsIdeHelper\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wulfheart\\LaravelActionsIdeHelper\\Tests\\": "tests"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wulfheart\\LaravelActionsIdeHelper\\LaravelActionsIdeHelperServiceProvider"
            ],
            "aliases": {
                "LaravelActionsIdeHelper": "Wulfheart\\LaravelActionsIdeHelper\\LaravelActionsIdeHelperFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
